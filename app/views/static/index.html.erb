<%= content_tag :div, id:"static-header" do %>
  <%= render 'static_header' %>
<% end %>
<% @projects.each do |p| %>
  <%= content_tag :div, class:"project-box" do %>
    <!-- <%= image_tag p.cover.url, class:"project-cover" %> -->
    <%= content_tag :h2, class:"project-title" do %>
      - <%= link_to p.title, p, class:"project-link #{p.category}-underline" %>
      <% if p.hidden? %>
        hidden
      <% end %>
    <% end %>
  <% end %>
<% end %>
<%= content_tag :div, will_paginate(@projects), class:"pagination-wrapper" %>
